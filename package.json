{
  "name": "grow-webpack-boilerplate",
  "version": "1.0.0",
  "description": "This repo tries to show a way to work with grow and webpack on a clean/module based way.",
  "scripts": {
    "start": "webpack --watch | ~/bin/grow run ./",
    "build": "NODE_ENV=production webpack -p --config webpack.config.js && ~/bin/grow build ./"
  },
  "keywords": [],
  "author": "@fonnegra",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  }
}
